<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chinasoftinternational.uav.mapper.CoordinateInfoMapper">

    <insert id="insertCoordinateInfo">
        INSERT INTO coordinate_info (plane_name,signal_index,message) VALUES (#{planeName},#{signalIndex},#{message})
    </insert>

</mapper>